{
  "name": "ng-ourmemorial",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build --prod --single-bundle --output-hashing none --vendor-chunk false",
    "build:staging": "ng build --configuration=staging",
    "build:kubernetes": "ng build --configuration=kubernetes",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "npm run cy:open",
    "concat": "concat -o ./dist/ng-ourmemorial/sdk.js ./dist/ng-ourmemorial/polyfills.js ./dist/ng-ourmemorial/main.js ./dist/ng-ourmemorial/4.js ./dist/ng-ourmemorial/6.js ./dist/ng-ourmemorial/5.js ./dist/ng-ourmemorial/3.js",
    "docker-build:kubernetes": "docker-compose -f docker-compose.kubernetes.yml build",
    "docker-tag:kubernetes": "docker tag adstate/java-hut:adstate-portal-frontend-kubernetes adstate/java-hut",
    "docker-push:kubernetes": "docker push adstate/java-hut:adstate-portal-frontend-kubernetes",
    "deploy:kubernetes": "npm run docker-build:kubernetes && npm run docker-tag:kubernetes && npm run docker-push:kubernetes",
    "docker-build:staging": "docker-compose -f docker-compose.staging.yml build",
    "docker-tag:staging": "docker tag adstate/java-hut:adstate-portal-frontend-staging adstate/java-hut",
    "docker-push:staging": "docker push adstate/java-hut:adstate-portal-frontend-staging",
    "deploy:staging": "npm run docker-build:staging && npm run docker-tag:staging && npm run docker-push:staging",
    "docker-build:prod": "docker-compose build",
    "docker-tag:prod": "docker tag adstate/adstate-portal-frontend-staging-release:latest adstate/adstate-portal-frontend-staging-release:%npm_package_version%",
    "docker-push:prod": "docker push adstate/java-hut:adstate-portal-frontend-staging-release:%npm_package_version%",
    "deploy:prod": "npm run docker-build:prod && npm run docker-tag:prod && npm run docker-push:prod",
    "deploy": "npm run concat && firebase deploy --only hosting",
    "cy:open": "cypress open",
    "sonar": "sonar-scanner"
  },
  "private": true,
  "dependencies": {
    "@adstate/flora": "^0.3.0",
    "@angular/animations": "~12.0.0",
    "@angular/cdk": "^12.2.1",
    "@angular/common": "~12.0.0",
    "@angular/compiler": "~12.0.0",
    "@angular/core": "~12.0.0",
    "@angular/elements": "^12.0.0",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/forms": "~12.0.0",
    "@angular/material": "^12.2.1",
    "@angular/platform-browser": "~12.0.0",
    "@angular/platform-browser-dynamic": "~12.0.0",
    "@angular/router": "~12.0.0",
    "@fortawesome/angular-fontawesome": "^0.8.2",
    "@ng-select/ng-select": "^6.1.0",
    "@ngneat/transloco": "^2.21.0",
    "@ngxs/logger-plugin": "^3.7.2",
    "@ngxs/router-plugin": "^3.7.2",
    "@ngxs/store": "^3.7.2",
    "@schematics/angular": "^9.1.0",
    "angular-pintura": "file:local_modules/angular-pintura",
    "bootstrap": "^5.0.1",
    "bootstrap-icons": "^1.5.0",
    "concurrently": "^6.0.0",
    "document-register-element": "^1.7.2",
    "filepond": "^4.28.2",
    "keycloak-angular": "^8.2.0",
    "keycloak-js": "^12.0.4",
    "ngx-bootstrap": "^6.2.0",
    "ngx-build-plus": "^11.0.0",
    "ngx-filepond": "^6.0.0",
    "ngx-image-cropper": "^3.3.5",
    "ngx-mask": "^12.0.0",
    "ngx-toastr": "^13.2.1",
    "pintura": "file:local_modules/pintura",
    "rxjs": "^7.0.1",
    "tslib": "^2.0.0",
    "vanilla-colorful": "^0.6.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.0",
    "@angular/cli": "~12.0.0",
    "@angular/compiler-cli": "~12.0.0",
    "@cucumber/cucumber": "^7.2.1",
    "@cypress/webpack-preprocessor": "^5.6.0",
    "@ngxs/devtools-plugin": "^3.7.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "concat": "^1.0.3",
    "cypress": "^6.8.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "sonar-scanner": "^3.1.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.2.4"
  }
}
