/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@app/env": [
        "src/environments/environment"
      ],
      "@app/config": [
        "src/app/config/app.config"
      ],
      "@app/core": [
        "src/app/core/index"
      ],
      "@app/layout": [
        "src/app/layout/index"
      ],
      "@app/core/*": [
        "src/app/core/*"
      ],
      "@app/shared": [
        "src/app/shared/index"
      ],
      "@app/shared/*": [
        "src/app/shared/*"
      ]
    },
    "allowJs": true,
    "jsx": "react-jsx",
  }
}
